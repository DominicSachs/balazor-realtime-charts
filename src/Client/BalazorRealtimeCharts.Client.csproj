    <ImplicitUsings>enable</ImplicitUsings>
    <AssemblyName>$(AssemblyName)</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="7.0.1" />
